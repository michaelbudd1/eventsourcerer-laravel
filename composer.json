    "type": "library",
    "autoload": {
        "psr-4": {
            "Eventsourcerer\\EventSourcererLaravel\\": "src/"
        }
    },
    "authors": [
        {
            "name": "michaelbudd",
            "email": "michaelbudd6@gmail.com"
        }
